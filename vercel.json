{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "MY_ENV_VARIABLE": "value_here"
    },
    "cleanUrls": true, 
    "trailingSlash": false
  }
  